@model IEnumerable<Chat>

<div class="rooms-list">
    @if(Model.Count() > 0)
    {
        @foreach(var chat in Model)
        {
            <a class="room-button-join" href="@Url.Action("AddToChat", "Home", new {chatId = chat.Id})">
                @chat.Name
            </a>
        }
    }
</div>



